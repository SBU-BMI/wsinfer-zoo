{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Schema for WSInfer Model Zoo registry file (wsinfer-zoo-registry.json)",
  "type": "object",
  "properties": {
    "models": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string"
          },
          "hf_repo_id": {
            "type": "string"
          },
          "hf_revision": {
            "type": "string"
          }
        },
        "required": [
          "description",
          "hf_repo_id",
          "hf_revision"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "models"
  ],
  "additionalProperties": false
}
